{
    "name": "lezhnev74/ddd-generator",
    "description": "DDD Generator for 3-layered applications",
    "keywords": [
        "ddd",
        "clean-architecture",
        "php",
        "generator"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitriy Lezhnev",
            "email": "lezhnev.work@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "beberlei/assert": "^2.7",
        "symfony/console": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "DDDGen\\": "src/domain",
            "DDDGenApp\\": "src/app",
            "DDDGen\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/dddtool"
    ]
}
